{
  "mcpServers": {
    "asset-gen": {
      "command": "node",
      "args": ["tools/asset-gen/index.js"],
      "env": {
        "REPLICATE_API_TOKEN": "${REPLICATE_API_TOKEN}"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgres://postgres:postgres@localhost:5432/huishype"
      ]
    }
  }
}